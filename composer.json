{
	"name": "pspepp3r/hng-backend-3",
	"description": "A backend application for HNG13 using Slim Framework",
	"type": "project",
	"license": "MIT",
	"authors": [
		{
			"name": "pspepp3r",
			"email": "prosperpepple12@gmail.com"
		}
	],
	"require": {
		"slim/slim": "^4.0",
		"slim/psr7": "^1.6",
		"slim/http": "^1.3",
		"monolog/monolog": "^2.0",
		"predis/predis": "^3.2",
		"vlucas/phpdotenv": "^5.5",
		"guzzlehttp/guzzle": "^7.0",
		"php-di/php-di": "*",
		"ramsey/uuid": "^4.9",
		"theodo-group/llphant": "^0.10.1"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.0"
	},
	"autoload": {
		"psr-4": {
			"Src\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Src\\Tests\\": "tests/"
		}
	},
	"config": {
		"allow-plugins": {
			"php-http/discovery": true
		}
	}
}
